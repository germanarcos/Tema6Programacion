
public class CuentaCorriente {
	/**
	 * @author Germán Arcos
	 */
	private String numCuenta;
	private double saldo;
	public void CuentaCorriente(String cuenta, double cantidad){
		/**
		 * @param numCuenta: String que contiene el número de cuenta
		 * @param saldo: La cantidad de dinero disponible en la cuenta
		 */
		numCuenta=cuenta;
		saldo=cantidad;
	}
	public void ingresaEfectivo (double cantidad){
		saldo+=cantidad;
	}
	public boolean retirarEfectivo (double cantidad){
		/**
		 * @author Germán Arcos
		 * @return Devuelve un booleano, verdadero si ha conseguido realizar la operación,
		 * falso si no, si consigue realizarla hace el cálculo
		 */
		if(saldo>=cantidad){
			saldo-=cantidad;
			return true;
		} 
		else{
			return false;
		}
	}
	public void visualiza(){
		System.out.println("El número de cuenta es: "+numCuenta);
		System.out.println("El saldo es:" +saldo);
	}
}
