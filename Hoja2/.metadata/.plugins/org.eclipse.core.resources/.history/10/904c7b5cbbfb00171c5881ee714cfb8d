
public class Libro {
	private String titulo;
	private Autor autor;
	private Double precio;
	private int cantidad;

	Libro(String titulo, Autor autor, double precio) {
		this.titulo = titulo;
		this.autor = autor;
		this.precio = precio;
	}

	Libro(String titulo, Autor autor, double precio, int cantidad) {
		this.titulo = titulo;
		this.autor = autor;
		this.precio = precio;
		this.cantidad = cantidad;

	}
}
